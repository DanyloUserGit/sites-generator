    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.19",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.19",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/typeorm": "^11.0.0",
    "@octokit/rest": "^18.12.0",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.0",
    "globby": "^12.2.0",
    "iconv-lite": "^0.7.0",
    "ioredis": "^5.6.1",
    "iso-639-1": "^3.1.5",
    "joi": "^17.13.3",
    "jsdom": "^27.0.0",
    "node-fetch": "^2.7.0",
    "openai": "^5.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "simple-git": "^3.28.0",
    "tldts": "^7.0.11",
    "typeorm": "^0.3.25",
    "unsplash-js": "^7.0.19"
